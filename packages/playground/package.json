{
  "name": "pro-playground",
  "version": "5.0.42",
  "private": true,
  "scripts": {
    "watch": "sleep 10 && node scripts/start.js",
    "build": "vite build",
    "start": "vite serve",
    "unit": "node scripts/test.js",
    "test": "echo 'no test in wix CI'",
    "deploy": "node scripts/build.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "yarn run lint --fix",
    "start-playground": "yarn start"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.19.1",
    "css-animation": "1.5.0",
    "deep-equal": "^2.1.0",
    "firebase": "^8.2.4",
    "image-client-api": "^1.3130.0",
    "lodash.clonedeep": "^4.5.0",
    "moment": "2.29.2",
    "pro-gallery": "workspace:*",
    "pro-gallery-blueprints": "workspace:*",
    "pro-gallery-lib": "workspace:*",
    "pro-layouts": "workspace:*",
    "re-resizable": "^6.8.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-color": "^2.17.3",
    "react-dom": "^16.8.6",
    "react-syntax-highlighter": "^15.4.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.0.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-watch-typeahead": "^0.6.0",
    "node-forge": "1.3.0",
    "prettier": "^2.8.1",
    "react-dev-utils": "^12.0.0",
    "vite": "4.1.2",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-svgr": "2.4.0"
  },
  "resolutions": {
    "css-animation": "1.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "homepage": "https://pro-gallery.surge.sh/"
}
