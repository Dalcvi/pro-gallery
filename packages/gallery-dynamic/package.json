{
  "private": false,
  "name": "pro-gallery-dynamic",
  "version": "4.0.15-beta.254",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/pro-gallery.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm-types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "npm run start",
    "build": " tsc && tsc -p tsconfig-esm.json && npm run css-compile",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css src/components/styles/gallery.scss:dist/statics/main.css",
    "start": "concurrently \"npm run css-watch\" \"npm run code-watch\"",
    "code-watch": "tsc && tsc -w -p tsconfig-esm-dev.json",
    "unit": "jest --testPathIgnorePatterns '.*/e2e/.*.e2e.spec.js' --silent",
    "test": "echo 'no test in wix CI'",
    "lint": "eslint \"{src,tests}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@vimeo/player": "2.8.2",
    "hls.js": "^1.1.5",
    "pro-gallery-blueprints": "4.0.15-beta.254",
    "pro-gallery-lib": "4.0.15-beta.254",
    "pro-layouts": "4.0.15-beta.254",
    "react-player": "^2.6.2",
    "framer-motion": "6.2.8"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "^7.5.5",
    "@types/react": "^16.14.6",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babelify": "^10.0.0",
    "chai": "^4.2.0",
    "chai-spies": "^0.7.1",
    "concurrently": "^6.1.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.0",
    "jest-image-snapshot": "^4.5.1",
    "jest-stare": "^2.2.0",
    "prettier": "^2.1.2",
    "puppeteer": "^1.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass": "^1.29.0",
    "sinon": "^6.0.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.1.0",
    "typescript": "4.1.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/drivers/mocks/stylesImportMock.js"
    },
    "reporters": [
      "default",
      "<rootDir>/tests/drivers/customReporter.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist",
      "<rootDir>/target"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/drivers/mocks/jest-setup.js",
      "<rootDir>/tests/environment-setup.js"
    ]
  }
}
